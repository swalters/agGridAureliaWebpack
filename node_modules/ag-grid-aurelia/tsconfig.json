{
  "compilerOptions": {
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "target": "es5",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "noEmitOnError": false,
    "outDir": "lib/",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "core-js"
    ]
  },
  "exclude": [
    "node_modules",
    "lib"
  ]
}